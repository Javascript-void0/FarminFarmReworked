on spawn:
	set {_p} to number of players
	set {_d} to number of entities in "world" - {_p}
	if {_d} is greater than 10:
		cancel event
	if block below event-location is not grass block:
		cancel event
on spawn of skeleton:
	boneArcher(event-location)
	boneWarrior(event-location)
	delete event-entities
on spawn of zombie:
	orcCaptain(event-location)
	orcWarrior(event-location)
	delete event-entities

every 10 seconds in "world":
	loop all entities:
		loop-entity is not a player
		if "%region at loop-entity%" does not contain "spawn":
			delete loop-entity