on combust:
	if event-entity is zombie or skeleton:
		cancel event

function boneWarrior(l: location):
	spawn skeleton at {_l}
	set name of last spawned entity to "&8[&e:L100&8] &fBone:Warrior &7(&c1200&7)"
	set max health of last spawned entity to 1200
	set health of last spawned entity to 1200
	set {_helmet} to bone
	set {_chestplate} to iron chestplate
	set {_leggings} to leather leggings
	dye {_leggings} white
	set {_boots} to iron boots
	set {_weapon} to bone
	enchant {_helmet} with thorns
	enchant {_chestplate} with protection 5, fire protection 3, blast protection 3, unbreaking 5, and thorns
	enchant {_leggings} with protection 5, fire protection 3, blast protection 3, unbreaking 5, and thorns
	enchant {_boots} with protection 5, fire protection 3, blast protection 3, unbreaking 5, and thorns
	enchant {_weapon} with sharpness 7
	equip last spawned entity with {_chestplate}, {_leggings}, and {_boots}
	set helmet of last spawned entity to {_helmet}
	set held item of last spawned entity to {_weapon}

